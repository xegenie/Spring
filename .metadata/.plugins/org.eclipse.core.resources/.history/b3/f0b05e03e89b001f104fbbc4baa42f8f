package com.aloha.spring.dto;

public class Board {

	private String title;
	private String writer;
	private String content;
	
	public Board() {
		this.title = "제목없음";
		this.writer= "작성자없음";
		this.content = "내용없음";
	}

	public Board(String title, String writer, String content) {
		super();
		this.title = title;
		this.writer = writer;
		this.content = content;
	}
	
}
